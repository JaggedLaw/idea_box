<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Login Page</title>
  </head>
  <body>
      <%= form_for(:session, url: login_path) do |f| %>
        <%= f.label :first_name, "Username" %>
        <%= f.text_field :first_name %>
        <%= f.label :password %>
        <%= f.text_field :password %>
        <%= f.submit "Login" %>
      <% end  %>
  </body>
</html>
